
;; Architecture extensions
#include weak "..\ARM\ARMv7-M-ETM.ddf"
#include weak "..\ARM\ARMv7-M-TPIU.ddf"
#include weak "..\ARM\ARMv7-M-MPU.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  ROM              Memory      0x1FFFD800  0x1FFFF7FF  R  
Memory =  OptionsROM       Memory      0x1FFFF800  0x1FFFFFFF  R  
Memory =  PeriphAPB1       Memory      0x40000000  0x40009FFF  W  
Memory =  PeriphAPB2       Memory      0x40010000  0x40016BFF  W  
Memory =  PeriphAHB1       Memory      0x40020000  0x400243FF  W  
Memory =  PeriphAHB2       Memory      0x48000000  0x48001FFF  W  
Memory =  PeriphAHB3       Memory      0x50000000  0x500007FF  W  
Memory =  PeriphAHB4       Memory      0x60000000  0xA0000FFF  W  
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W  
Memory =  Remap            Memory      0x00000000  0x0003FFFF  R  
Memory =  Flash            Memory      0x08000000  0x0803FFFF  R  
Memory =  CCMRAM           Memory      0x10000000  0x10001FFF  RW 
Memory =  RAM              Memory      0x20000000  0x20009FFF  RW 

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = STM32F3x8.svd
