#include weak "..\ARM\ARMv7-M-MPU.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  ROM              Memory      0x1FFF0000  0x1FFFF83F  R  
Memory =  PeriphAPB1       Memory      0x40000000  0x400097FF  W  
Memory =  PeriphAPB2-1     Memory      0x40010000  0x40013BFF  W  
Memory =  PeriphAPB2-2     Memory      0x40014000  0x400164FF  W  
Memory =  PeriphAHB1       Memory      0x40020000  0x400243FF  W  
Memory =  Periph0_BitBand  Memory      0x42000000  0x4212FFFF  W  
Memory =  Periph1_BitBand  Memory      0x42200000  0x422C7FFF  W  
Memory =  Periph2_BitBand  Memory      0x42400000  0x42487FFF  W  
Memory =  PeriphAHB2       Memory      0x48000000  0x50060BFF  W  
Memory =  ExtDev           Memory      0x60000000  0x8FFFFFFF  W  
Memory =  PeriphAHB3       Memory      0xA0000000  0xA00013FF  W  
Memory =  QSPI_Flash       Memory      0x90000000  0x9FFFFFFF  R  
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W  
Memory =  RemapMem         Memory      0x00000000  0x000FFFFF  R  
Memory =  Flash            Memory      0x08000000  0x080FFFFF  R  
Memory =  RAM0             Memory      0x10000000  0x10007FFF  RW 
Memory =  RAM1             Memory      0x20000000  0x20017FFF  RW 
Memory =  RAM1_BitBand     Memory      0x22000000  0x222FFFFF  RW 

TrustedRanges = true
UseSfrFilter = true

