
;; Architecture extensions
#include weak "..\ARM\ARMv7-M-TPIU.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  Periph           Memory      0x40000000  0x4000FFFF  W  
Memory =  Periph-BitBand   Memory      0x42000000  0x421FFFFF  W  
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W  
Memory =  RemapMem         Memory      0x00000000  0x0003FFFF  R  
Memory =  Flash            Memory      0x08000000  0x0803FFFF  R  
Memory =  RAM              Memory      0x20000000  0x20003FFF  RW 
Memory =  RAM-BitBand      Memory      0x22000000  0x2207FFFF  RW 
Memory =  ROM              Memory      0x08040000  0x08044FFF  R  

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = STM32W108.svd
