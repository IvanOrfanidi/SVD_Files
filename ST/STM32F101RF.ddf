
;; Architecture extensions
#include weak "..\ARM\ARMv7-M-ETM.ddf"
#include weak "..\ARM\ARMv7-M-TPIU.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  Periph0          Memory      0x40000000  0x400233FF  W  
Memory =  Periph-BitBand   Memory      0x42000000  0x42467FFF  W  
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W  
Memory =  ROM              Memory      0x1FFFF000  0x1FFFF83F  R  
Memory =  ExtDev           Memory      0x60000000  0x9FFFFFFF  W  
Memory =  Periph1          Memory      0xA0000000  0xA0000FFF  W  
Memory =  RemapMem         Memory      0x00000000  0x000BFFFF  R  
Memory =  Flash            Memory      0x08000000  0x080BFFFF  R  
Memory =  RAM              Memory      0x20000000  0x20013FFF  RW 
Memory =  RAM-BitBand      Memory      0x22000000  0x2227FFFF  RW 

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = STM32F101.svd
