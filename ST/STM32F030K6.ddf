
;; Architecture extensions
#include weak "..\ARM\ARMv6-M-SYST.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  ROM              Memory      0x1FFFC800  0x1FFFFFFF  R  
Memory =  PeriphAPB-1      Memory      0x40000000  0x400073FF  W  
Memory =  PeriphAPB-2      Memory      0x40010000  0x40015BFF  W  
Memory =  PeriphAHB1       Memory      0x40020000  0x400233FF  W  
Memory =  PeriphAHB2       Memory      0x48000000  0x480017FF  W  
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W  
Memory =  RemapMem         Memory      0x00000000  0x00007FFF  R  
Memory =  Flash            Memory      0x08000000  0x08007FFF  R  
Memory =  RAM              Memory      0x20000000  0x20000FFF  RW 

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = STM32F0x0.svd
